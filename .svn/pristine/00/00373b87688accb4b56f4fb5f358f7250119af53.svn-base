/*tooltip */
.tooltip {position:relative; display:inline-block; line-height:20px; font-size:13px; text-align:left; vertical-align:middle}
.tooltip * {word-break:keep-all; word-wrap:break-word}
.tooltip .green {color:#8eecc1}
