<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<!-- 
	[마이바티스] 스프링에서 "_"를 사용한 컬럼명을 사용 시(BOOK 테이블의 BOOK_ID)
	카멜케이스로 읽어줌(bookId), INSERT_DATE => insertDate
	ex) 테이블 컬러명이 member_id인 경우 jsp화면단에서 이 값을 사용 시 memberId로 사용
	-->
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	<!-- 자주 사용하는 타입의 별칭을 세팅 -->
	<typeAliases>
		<typeAlias type="com.homecat.sweethome.vo.board.BoardVO" alias="boardVO" />
		<typeAlias type="com.homecat.sweethome.vo.board.ReplyVO" alias="replyVO" />
		<typeAlias type="com.homecat.sweethome.vo.survey.SurveyVO" alias="surveyVO" />
		<typeAlias type="com.homecat.sweethome.vo.survey.SurveyDetailVO" alias="surveyDetailVO" />
		<typeAlias type="com.homecat.sweethome.vo.calendar.CalendarVO" alias="calendarVO" />
		<typeAlias type="com.homecat.sweethome.vo.member.MemberVO" alias="memberVO" />
		<typeAlias type="com.homecat.sweethome.vo.publicFacilities.BookVO" alias="bookVO" />
		<typeAlias type="com.homecat.sweethome.vo.publicFacilities.BookLoanVO" alias="bookLoanVO" />
		<typeAlias type="com.homecat.sweethome.vo.waste.WasteVO" alias="wasteVO" />
		<typeAlias type="com.homecat.sweethome.vo.comm.CommCodeVO" alias="commCodeVO" />
		<typeAlias type="com.homecat.sweethome.vo.comm.CommDetailVO" alias="commDetailVO" />
		<typeAlias type="com.homecat.sweethome.vo.attach.AttachVO" alias="attachVO" />
		<typeAlias type="com.homecat.sweethome.vo.attach.AttachDetailVO" alias="attachDetailVO" />
		<typeAlias type="com.homecat.sweethome.vo.room.RoomVO" alias="roomVO" />
	</typeAliases>
</configuration>


